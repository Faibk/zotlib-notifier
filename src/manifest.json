{
	"manifest_version": 2,
	"name": "Custom Zotlit Notifier",
	"version": "0.1",
	"description": "Notifies zotlit in obsidian about tab and annotation changes",
	"homepage_url": "https://github.com/Faibk/zotlit-notifier",
	"applications": {
		"zotero": {
			"id": "zotlit-notifier@faibk.github.io",
			"update_url": "https://github.com/Faibk/zotlit-notifier/blob/master/update.json",
			"strict_min_version": "6.999",
			"strict_max_version": "7.0.*"
		}
	}
}
